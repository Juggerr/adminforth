{
  "name": "adminforth",
  "version": "1.3.18",
  "description": "OpenSource Vue3 powered forth-generation admin panel",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "---prepareDist": "cp -rL spa dist/",
    "rollout": "tsc && npm version patch && npm publish && npm run rollout-doc",
    "rollout-doc": "cd documentation && npm run build && npm run deploy",
    "docs": "typedoc",
    "postinstall": "cd ./spa/src/ && test ! -d ./types && ln -sf ../../types ./types || echo 'types linked'"
  },
  "author": "devforth.io",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@clickhouse/client": "^1.4.0",
    "better-sqlite3": "^10.0.0",
    "dayjs": "^1.11.11",
    "express": "^4.21.0",
    "filewatcher": "^3.0.1",
    "fs-extra": "^11.2.0",
    "fuse.js": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "6.6",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "typescript": "^5.4.5"
  }
}
